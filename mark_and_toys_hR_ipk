public static int maximumToys(List<Integer> prices, int k) {
        Collections.sort(prices);
        int sum = 0;
        int maxnt = 0;
        for(int i = 0 ; i < prices.size() ; i++) {
            sum = sum + prices.get(i);
            if(sum > k) {
                break;
            } else {
                maxnt++;
            }
        }
        return maxnt;
    }
