public static int minimumAbsoluteDifference(List<Integer> arr) {
        int min = Integer.MAX_VALUE;
        Collections.sort(arr);
        for(int i = 0 ; i < arr.size()-1 ; i++) {
            int c_min = Math.abs(arr.get(i) - arr.get(i+1));
            min = Math.min(min, c_min);
        }
        return min;
    }
