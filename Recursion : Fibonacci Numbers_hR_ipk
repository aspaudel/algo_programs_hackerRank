static int v = 2, sum = 0, sum1 = 1, sum2 = 0;
    public static int fibonacci(int n) {
        if(n == 0) {
            return 0;
        }
        if(n == 1) {
            sum = 1;
            sum1 = sum;
            return sum;
        } else {
            sum = sum1 + sum2;
            sum2 = sum1;
            sum1 = sum;
            v++;
            if(v <= n) {
                sum = fibonacci(n);    
            }
        }  
        return sum; 
    } 
