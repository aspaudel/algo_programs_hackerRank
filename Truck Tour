public static int truckTour(List<List<Integer>> petrolpumps) {
        int sum = 0;
        int f = 0;
        for(int i = 0 ; i < petrolpumps.size() ; i++) {
            sum += petrolpumps.get(i).get(0);
            sum -= petrolpumps.get(i).get(1);
            if(sum < 0) {
                f = i + 1;
                sum = 0;
            }        
        }
        return f;
    }
